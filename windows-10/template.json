{
  "builders": [
    {
      "type": "virtualbox-iso",
      "boot_wait": "2m",
      "disk_size": "61440",
      "floppy_files": [
        "unattend/Autounattend.xml",
        "scripts/fixnetwork.ps1",
        "scripts/microsoft-updates.bat",
        "scripts/openssh.ps1",
        "scripts/oracle-cert.cer",
        "scripts/win-updates.ps1"
      ],
      "guest_additions_mode": "disable",
      "guest_os_type": "Windows10_64",
      "headless": false,
      "iso_checksum": "56ab095075be28a90bc0b510835280975c6bb2ce",
      "iso_checksum_type": "sha1",
      "iso_url": "http://care.dlservice.microsoft.com/dl/download/C/3/9/C399EEA8-135D-4207-92C9-6AAB3259F6EF/10240.16384.150709-1700.TH1_CLIENTENTERPRISEEVAL_OEMRET_X64FRE_EN-US.ISO",
      "shutdown_command": "shutdown /s /t 10 /f /d p:4:1 /c \"Packer Shutdown\"",
      "ssh_password": "vagrant",
      "ssh_username": "vagrant",
      "ssh_port": "22",
      "ssh_wait_timeout": "6h",
      "vm_name": "windows_10",
      "vboxmanage": [
        ["modifyvm", "{{.Name}}", "--cpus", "2"],
        ["modifyvm", "{{.Name}}", "--memory", "2048"]
      ],
      "virtualbox_version_file": ".vbox_version"
    }
  ],
  "post-processors": [
    {
      "type": "vagrant",
      "output": "windows_10.box",
      "vagrantfile_template": "vagrant/vagrantfile.tpl"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "remote_path": "/tmp/script.bat",
      "execute_command": "{{.Vars}} cmd /c C:/Windows/Temp/script.bat",
      "override": {
        "virtualbox-iso": {
          "scripts": [
            "scripts/base.bat",
            "scripts/vagrant.bat",
            "scripts/virtualbox.bat",
            "scripts/cleanup.bat",
            "scripts/minimize.bat"
          ]
        }
      },
      "pause_before": "5s"
    }
  ]
}
