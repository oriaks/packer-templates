{
  "builders": [
    {
      "type": "virtualbox-iso",
      "boot_command": [
        "<tab> text ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/ks.cfg <enter><wait>"
      ],
      "boot_wait": "10s",
      "disk_size": "40520",
      "guest_os_type": "RedHat_64",
      "headless": false,
      "http_directory": "kickstart",
      "iso_checksum": "b9694b643ce896b41316d4f131ee0f70",
      "iso_checksum_type": "md5",
      "iso_url": "http://mirror.clearos.com/clearos/7/iso/x86_64/ClearOS-netinst-x86_64-7.1.0.iso",
      "shutdown_command": "echo 'vagrant'|sudo -S /sbin/halt -h -p",
      "ssh_username": "vagrant",
      "ssh_password": "vagrant",
      "ssh_port": "22",
      "ssh_wait_timeout": "10000s",
      "vm_name": "clearos-7",
      "guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
      "vboxmanage": [
        [ "modifyvm", "{{.Name}}", "--cpus", "1" ],
        [ "modifyvm", "{{.Name}}", "--memory", "512" ]
      ],
      "virtualbox_version_file": ".vbox_version"
    }
  ],
  "post-processors": [
    {
      "type": "vagrant",
      "output": "clearos-7.box"
    }
  ],
  "provisioners": [
    {
      "type": "shell",
      "execute_command": "echo 'vagrant'|sudo -S sh '{{.Path}}'",
      "override": {
        "virtualbox-iso": {
          "scripts": [
            "scripts/base.sh",
            "scripts/vagrant.sh",
            "scripts/virtualbox.sh",
            "scripts/cleanup.sh",
            "scripts/minimize.sh"
          ]
        }
      },
      "pause_before": "5s"
    }
  ]
}
